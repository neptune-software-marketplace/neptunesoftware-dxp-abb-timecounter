						{
							"fieldNo": "f0a52946-86dd-4f63-efd8-6d3daac38f6f",
							"fieldName": "How_to_Use",
							"fieldParent": "921cd654-1377-40ef-8bcb-cc1a8905d7c0",
							"fieldType": "neptune.Script",
							"script": [
								"==OBJECT STRING==",
								"// MainContainerTimer is the Horizontal COntainer can be placed under any UI COntainer",
								"// ContentTimer is the actual container for the Hours, Minutes and Seconds Text",
								"// TimeCSSContainer has local styles for the counter",
								"",
								"// you can place MainContainerTimer as is inside sap.m.Page or sap.m.Panel and the contents",
								"// will be aligned to Horizontally Centre",
								"",
								"// If you want to use just the Time inside your own container, then use ContentTimer only"
							],
							"request": [],
							"response": [],
							"attributes": [],
							"data": []
						},
						{
							"fieldNo": "dafaa225-1047-4ce3-b23a-e820d76b1fab",
							"fieldName": "TimerCSSContainer",
							"fieldParent": "921cd654-1377-40ef-8bcb-cc1a8905d7c0",
							"fieldType": "sap.ui.core.HTML",
							"script": "",
							"request": [],
							"response": [],
							"attributes": [],
							"data": [
								{
									"fieldNo": "bd9033de-100b-4996-8bf5-0c7a8ea4e345",
									"fieldName": "htmlStyleTimer",
									"fieldParent": "dafaa225-1047-4ce3-b23a-e820d76b1fab",
									"fieldType": "neptune.Html",
									"script": [
										"==OBJECT STRING==",
										"<style type=\"text/css\" media=\"all\">",
										".countStyle{",
										"    font-size: 40px !important;",
										"    /*text-shadow: 2px 2px #636363 !important;*/",
										"    font-family: monospace !important;",
										"    color: #fff !important;",
										"}",
										"",
										".countBG{",
										"   background-color: #000000ba !important;",
										"}",
										"</style>"
									],
									"request": [],
									"response": [],
									"attributes": [],
									"data": []
								}
							]
						},
						{
							"fieldNo": "a5a13158-cf07-45dc-a55b-ae436c36f260",
							"fieldName": "TimerInit",
							"fieldParent": "921cd654-1377-40ef-8bcb-cc1a8905d7c0",
							"fieldType": "neptune.Script",
							"script": [
								"==OBJECT STRING==",
								"// Initialize Timer",
								"sap.ui.getCore().attachInit(function(data) {",
								"",
								"    setTimeout(function() {",
								"        ",
								"        // Increament Time every second",
								"        var myInterval = setInterval(function() {",
								"",
								"            var date = new Date();",
								"",
								"            var hour = date.getHours();",
								"            var min = date.getMinutes();",
								"            var sec = date.getSeconds();",
								"",
								"            if (hour < 10) {",
								"                txtHours.setText('0' + hour);",
								"            } else {",
								"                txtHours.setText(hour);",
								"            }",
								"",
								"            if (min < 10) {",
								"                txtMins.setText('0' + min);",
								"            } else {",
								"                txtMins.setText(min);",
								"            }",
								"",
								"            if (sec < 10) {",
								"                txtSec.setText('0' + sec);",
								"            } else {",
								"                txtSec.setText(sec);",
								"            }",
								"",
								"        }, 1000);",
								"",
								"    }, 100);",
								"",
								"});"
							],
							"request": [],
							"response": [],
							"attributes": [],
							"data": []
						}
					]
				}
			]
